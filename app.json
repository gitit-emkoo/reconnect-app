{
  "expo": {
    "name": "리커넥트",
    "slug": "reconnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "reconnectwebview",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "리커넥트 - 감정 기록, 관계 돌봄 부부 갈등 케어 앱",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.reconnect.kwcc",
      "buildNumber": "1",
      "infoPlist": {
        "CFBundleDisplayName": "리커넥트",
        "CFBundleName": "리커넥트",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.reconnect.kwcc",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "384a9386-5865-455b-bd9b-5da26351e24e"
      }
    }
  }
}
